<div style="background-color: rgb(237, 237, 237); color: #292929; display: flex; justify-content: space-between;
    flex-direction: column; height: 100%; line-height: 1.2; padding: 2.5rem 4.5rem 5rem 5rem">
  <p style="font-size: 5rem;">{{ title | apostrophes }}</p>
  {# <div style="font-size: 2rem; display: flex; margin-bottom: auto">
    {% for tag in tags | filterTagList %}
      <span style="margin-right: 1.5rem">#{{tag | lower}}</span>
    {% endfor %}
  </div> #}
  <div style="display: flex; gap: 2rem; align-items: flex-end;">
    <img
      width="155"
      height="155"
      src="data:image/jpeg;base64,"
      alt="Red dot"
      style="border: 0.25em solid hsl(195, 60%, 59%); border-radius: 1rem; box-shadow: 0.5px 1px 0 0 hsl(195, 60%, 59%), 1.5px
        2px 0 0 hsl(195, 60%, 59%), 2.5px 3px 0 0 hsl(195, 60%, 59%), 3.5px 4px 0 0 hsl(195, 60%, 59%), 4.5px 5px 0 0 hsl(195,
        60%, 59%), 5.5px 6px 0 0 hsl(195, 60%, 59%); position: relative; top: 0.5rem;"/>
    <div style="display: flex; flex-direction: column">
      <p style="font-size: 2.5rem; color: #6B6B6B; margin-bottom: 1rem;">{{ date | readableDate }}</p>
      <span style="border-radius: 1rem; padding: 1rem 1.5rem; width: 25rem; font-size: 3rem; background-color: hsl(195, 53%, 79%);
          box-shadow: 1px 2px 0 0 hsl(195, 60%, 59%), 3px 4px 0 0 hsl(195, 60%, 59%), 5px 6px 0 0 hsl(195, 60%, 59%), 7px 8px 0 0
          hsl(195, 60%, 59%), 9px 10px 0 0 hsl(195, 60%, 59%), 11px 12px 0 0 hsl(195, 60%, 59%)">
        bastiencalou.fr</span>
    </div>
  </div>
</div>